        font-weight: normal;
        font-style: normal;
    }

    * {
        font-family: 'NEXON Lv1 Gothic OTF';
    }

    /* 채팅기능 css */
    #chat_btn {
        position: fixed;
        right: 50px;
        bottom: 50px;
        width: 50px;
        height: 50px;
    }

    .chat_btn {
        display: none;
    }

    #chat_btn:hover {
        cursor: pointer;
    }

    .close {
        width: 50px;
        height: 50px;
        position: absolute;
        right: 35px;
        top: -5px;
        z-index: 1;
        background-image: url("https://s1.daumcdn.net/cfs.tistory/custom/blog/204/2048858/skin/images/close.png");
        background-size: 50%;
        background-repeat: no-repeat;
        background-position: center;
        cursor: pointer;
    }

    #menu {
        width: 350px;
        height: 100vh;
        position: fixed;
        top: 0px;
        right: -352px;
        z-index: 10;
        border: 1px solid #c9c9c9;
        background-color: white;
        text-align: center;
        transition: All 0.2s ease;
        -webkit-transition: All 0.2s ease;
        -moz-transition: All 0.2s ease;
        -o-transition: All 0.2s ease;
        overflow: scroll;
    }

    /* 메뉴 스크롤바 숨기기 */

    #menu {
        -ms-overflow-style: none;
    }

    #menu::-webkit-scrollbar {
        display: none;
    }

    #menu.open {
        right: 0px;
    }

    .nav-item {
        margin-right: 40px;
    }

    .card {
        margin: auto;
        text-align: center;
    }

    .card-body>a {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }

    .iq-media-group .iq-media img {
        border-right: 2px solid;
    }

    .iq-media-group.me-3 {
        margin-left: 20px;

    }

    .avatar-40 {
        height: 40px;
        width: 40px;
        line-height: 40px;
        margin-left: -20px;
    }

    .img-fluid.avatar-40.rounded-circle:hover {
        height: 42px;
        width: 42px;
    }

    /* 동호회 게시글 작성하기 버튼 색상 조절 */
    .bg-soft-primary {
        color: #48a3e6;
        background-color: rgba(80, 181, 255, .2) !important;
    }






    /* 우측 사이드바 회원검색 검색창 */
    .iq-search-bar .search-input {
        width: 100%;
        height: 2.5rem;
        padding: 0.313rem 2.5rem 0.313rem 0.938rem;
        -webkit-border-radius: 5px;
        border-radius: 5px;
        border: none;
        border: 1px solid #f1f1f1;
    }

    button,
    input,
    optgroup,
    select,
    textarea {
        margin: 0;
        font-family: inherit;
        font-size: inherit;
        line-height: inherit;
    }


    /* 우측 사이드바 회원검색 돋보기 아이콘 */
    .members-search .searchbox .search-link {
        top: 0.313rem;
    }

    .iq-search-bar .searchbox .search-link {
        position: absolute;
        right: 30px;
        top: 95px;
        font-size: 1rem;
    }


    /* 카드박스 css */
    .card {
        -webkit-box-shadow: 0 0 1.25rem 0 rgb(0 0 0 / 10%);
        box-shadow: 0 0 1.25rem 0rgba(0, 0, 0, .1);
        margin-bottom: 1rem;
        -webkit-border-radius: 5px;
        border-radius: 5px;
    }

    .card .card-header {
        margin-bottom: 0;
        -ms-flex-align: center !important;
        -webkit-box-align: center !important;
        align-items: center !important;
        border: 0;
        border-bottom: 1px solid #f1f1f1;
    }


    /* 댓글창 우측 이모티콘 */
    
    .comment-attagement {
        position: absolute;
        left: auto;
        right: 15px;
        font-size: 1.25rem;
    }

    .comment-attagement a {
        color: gray;
    }


    /* 좋아요 드롭다운 화살표 제거 */
    .dropdown-toggle::after {
        display: none;
    }

    /* 좋아요 이모티콘 모음 정렬 */
    .dropdown-menu.py-2.show {
        width: 270px;
    }

    /* 동호회 설정 텍스트 좌측정렬 */
    .flex-grow-1.ms-3 {
        text-align: left;
